<%=form_for(@song) do |f|%>
  <%=f.label :title%>
  <%=f.text_field :title, @song.title%>
  <%=f.label :release_year%>
  <%=f.text_field :release_year, @song.release_year%>
  <%=f.label :released%>
  <%=f.check_box :released, @song.released%>
  <%=f.label :genre%>
  <%=f.text_field :genre%>
  <%=f.label :artist_name%>
  <%=f.text_field :artist_name%>
  <%=f.submit%>
<%end%>
